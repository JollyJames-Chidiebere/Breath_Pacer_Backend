{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "NIXPACKS"
  },
  "deploy": {
    "startCommand": "python manage.py migrate --noinput && gunicorn Breath_Pacer_Backend.wsgi:application --bind 0.0.0.0:8000",
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 100
  }
}
